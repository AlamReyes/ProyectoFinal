#include <stdio.h>
int main()
{
    int num1, num2;
    // PREGUNTA POR DOS NÚMEROS
    printf("Ingresa dos números \n");
    // Lee los números del usuario
    scanf("%d %d", &num1, &num2);
    if(num1 > num2)
    {
        printf("%d es el mayor\n", num1);
    }
    else if (num2 > num1)
    {
        printf("%d es el mayor\n", num2);
    }
    else
    {
        printf("Son iguales\n");
    }
    return 0;
}
